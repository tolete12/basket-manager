CREATE TABLE "PLAYERS" (
	"player_id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
	"name_id"	INTEGER,
	"surname_id"	INTEGER,
	"birthdate_id"	INTEGER,
	"team_id"	INTEGER,

    "height" INTEGER,
    "weight" INTEGER,
	"intelligence" INTEGER,
	"positioning" INTEGER,
    "temperament" INTEGER,
    "leadership" INTEGER,
    "team_player" INTEGER,
    "aggression" INTEGER,
    "potential" INTEGER
    "mental_toughness" INTEGER,
    "health" INTEGER,
    "tactics" INTEGER,
    "speed"	INTEGER,
	"acceleration"	INTEGER,
	"natural_fitness"	INTEGER,
	"stamina"	INTEGER,
	"jumping"	INTEGER,
	"strength"	INTEGER,
	"balance" INTEGER,
	"side_scrolling" INTEGER,
	"dribbling" INTEGER,

	"layup"	INTEGER,
	"dunk"	INTEGER,
	"close_shooting"	INTEGER,
	"mid_range_shooting"	INTEGER,
	"three_shooting"	INTEGER,
	"corner_shooting" INTEGER,
	"free_throw" INTEGER,
	"passing"	INTEGER,
	"off_the_ball"	INTEGER,
	"low_post"	INTEGER,
	"high_post"	INTEGER,
	"def_rebounding" INTEGER,
	"off_rebounding" INTEGER,
	"stealing" INTEGER,
	"ball_defence" INTEGER,
	"off_ball_defence" INTEGER,
	"screening" INTEGER,
	"changing_screens" INTEGER,
	"handling" INTEGER,
	"blocking" INTEGER
);