CREATE TABLE "SEASONS" (
	"season_id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
	"name"	TEXT,
	"league_id"	INTEGER,
	"preseason_start_date_id"	INTEGER,
	"preseason_end_date_id"	INTEGER,
	"season_start_date_id"	INTEGER,
	"season_end_date_id"	INTEGER,
	"playoffs_start_date_id"	INTEGER,
	"playoffs_end_date_id"	INTEGER,
	"draft_date_id_1"	INTEGER,
	"draft_date_id_2"	INTEGER,
	"draft_date_id_3"	INTEGER,
	"whole_season_start_date_id"	INTEGER,
	"whole_season_end_date_id"	INTEGER,
	"draft_rounds" INTEGER,
	"salary_cap"	INTEGER,
	"mid_level"	INTEGER,
	"bi_annual"	INTEGER,
	"weeks" INTEGER,
	"min_bye_week"	INTEGER,
	"max_bye_week"	INTEGER,
	"all_star_start_date_id" INTEGER,
	"all_star_end_date_id" INTEGER
);
