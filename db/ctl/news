CREATE TABLE "NEWS" (
	"news_id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
	"team_id_1"	INTEGER,
	"team_id_2"	INTEGER,
	"team_id_3"	INTEGER,
	"team_id_4"	INTEGER,
	"league_id_1"	INTEGER,
	"league_id_2"	INTEGER,
	"text"	INTEGER,
	"action_id_1"	INTEGER,
	"action_id_2"	INTEGER,
	"action_id_3"	INTEGER
);